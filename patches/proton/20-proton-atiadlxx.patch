diff --git a/dlls/atiadlxx/Makefile.in b/dlls/atiadlxx/Makefile.in
new file mode 100644
index 00000000000..78af7168c20
--- /dev/null
+++ b/dlls/atiadlxx/Makefile.in
@@ -0,0 +1,5 @@
+MODULE = atiadlxx.dll
+EXTRADLLFLAGS = -mno-cygwin -Wb,--prefer-native
