+{
+    SpatialAudioStreamImpl *This = impl_from_ISpatialAudioObjectRenderStream(iface);
+    HRESULT hr;
+
+    TRACE("(%p)->()\n", This);
+
+    hr = IAudioClient_Start(This->client);
