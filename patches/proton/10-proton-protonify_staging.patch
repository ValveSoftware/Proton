     return info;
 }
 
-static const struct png_funcs png_funcs =
+int CDECL hack_unix_setenv(const char *name, const char *value, int overwrite)
+{
+    return setenv(name, value, overwrite);
+}
+
+static const struct png_funcs png_funcs_unix =
 {
     get_png_info,
-    load_png
+    load_png,
+    hack_unix_setenv,
 };
 
