+static WAVEFORMATEX *clone_fmtex(const WAVEFORMATEX *src)
+{
+    WAVEFORMATEX *r = heap_alloc(sizeof(WAVEFORMATEX) + src->cbSize);
+    memcpy(r, src, sizeof(WAVEFORMATEX) + src->cbSize);
+    return r;
