+
+enum TreeScope {
+    TreeScope_Element     = 0x01,
+    TreeScope_Children    = 0x02,
+    TreeScope_Descendants = 0x04,
+    TreeScope_Parent      = 0x08,
+    TreeScope_Ancestors   = 0x10,
+    TreeScope_SubTree     = TreeScope_Element | TreeScope_Children | TreeScope_Descendants,
+};
