+    PropertyConditionFlags_None       = 0x00,
+    PropertyConditionFlags_IgnoreCase = 0x01,
+};
+
+enum AutomationElementMode {
+    AutomationElementMode_None = 0x00,
+    AutomationElementMode_Full = 0x01,
+};
+
+cpp_quote( "#endif" )
+
+struct ExtendedProperty
