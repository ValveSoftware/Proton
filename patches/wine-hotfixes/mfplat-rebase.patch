+static BOOL raw_media_converter_init_gst(struct wg_parser *parser)
+{
+    BOOL video = parser->input_format.major_type == WG_MAJOR_TYPE_VIDEO;
+    struct wg_parser_stream *stream;
+    GstElement *convert, *resampler;
+    GstPad *their_src;
