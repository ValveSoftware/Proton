+HRESULT WINAPI MFGetStrideForBitmapInfoHeader(DWORD format, DWORD width, LONG *stride)
+{
+    TRACE("%u, %u, %p\n", format, width, stride);
+
+    switch(format)
+    {
+        case MAKEFOURCC('N','V','1','2'):
+        {
+            *stride = width;
+            break;
+        }
+        default:
+        {
+            FIXME("Unrecognized format %u\n", format);
+            return E_NOTIMPL;
+        }
+    }
+
+    return S_OK;
+}
