int
main ()
{
int result = 0;
#ifdef HAVE_GETDTABLESIZE
            int bad_fd = getdtablesize ();
#else
            int bad_fd = 1000000;
#endif
#ifdef FD_CLOEXEC
            if (fcntl (1, F_SETFD, FD_CLOEXEC) == -1)
              result |= 1;
#endif
            if (dup2 (1, 1) == 0)
              result |= 2;
#ifdef FD_CLOEXEC
